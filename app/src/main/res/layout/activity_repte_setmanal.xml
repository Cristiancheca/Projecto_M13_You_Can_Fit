<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RepteSetmanal">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REPTE SETMANAL"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="409dp"
        android:layout_height="657dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/ejercici1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="EJERCICI 1" />

        <Button
            android:id="@+id/ejercici2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="EJERCICI 2" />

        <Button
            android:id="@+id/ejercici3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="EJERCICI 3" />

        <Button
            android:id="@+id/ejercici4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="EJERCICI 4" />

        <Button
            android:id="@+id/ejercici5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="EJERCICI 5" />

        <Button
            android:id="@+id/ejercici6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="EJERCICI 6" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>